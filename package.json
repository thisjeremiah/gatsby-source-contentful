{
  "name": "gatsby-source-contentful",
  "version": "1.3.35",
  "description":
    "Gatsby source plugin for building websites using the Contentful CMS as a data source",
  "scripts": {
    "build": "./node_modules/.bin/babel src --out-dir . --ignore __tests__",
    "watch": "./node_modules/.bin/babel -w src --out-dir . --ignore __tests__",
